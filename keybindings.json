// Place your key bindings in this file to override the defaultsauto[]
[
	// Mode Changing
	 {
		"key": "backspace",
		"command": "dance.set.normal",
		"when": "editorTextFocus"
	},
	{
		"key": "u",
		"command": "dance.set.insert",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	
	// Navigation
	// Single increment
	{
		"key": "r",
		"command": "tabsanity.cursorRight",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	{
		"key": "g",
		"command": "tabsanity.cursorLeft",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	{
		"key": "t",
		"command": "cursorDown",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	{
		"key": "c",
		"command": "cursorUp",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	
	// Line increment 
	{
		"key": "space d",
		"command": "dance.goto.lineStart.nonBlank",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	{
		"key": "space shift+d",
		"command": "binaryJump.binaryLeftJump",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	
	{
		"key": "space s",
		"command": "dance.goto.lineEnd",
