	},
	{
		"key": "space shift+s",
		"command": "binaryJump.binaryRightJump",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	
	// Paragraph
	{
		"key": "s",
		"command": "block-travel.jumpDown",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	{
		"key": "d",
		"command": "block-travel.jumpUp",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},

	// Word
	{
		"key": "h",
		"command": "cursorWordLeft",
		"when": "editorTextFocus && dance.mode == 'normal'"
	},
	{
		"key": "shift+h",
		"command": "cursorWordPartLeft",
		"when": "editorTextFocus && dance.mode == 'normal'"
