		"editor.lineHighlightBackground": "#433F47",
		"editor.findMatchBackground": "#104956",
		"editor.rangeHighlightBackground": "#433F47",
		"editor.rangeHighlightBorder": "#9580FF",
		"editor.findMatchHighlightBackground": "#433F47",
