		"html",
		"javascript",
		"json",
		"scss"
	],
	"dimmer.enabled": true,
	"dimmer.opacity": 87,
	"editor.wordWrap": "on",
	// "editor.formatOnPaste": true,
	// "editor.formatOnSave": true,
	"dired.fixed_window": true,
	"jumpToHint.theme.backgroundColor": "#9eddff",
	"jumpToHint.theme.fontColor": "#1d1e26",
