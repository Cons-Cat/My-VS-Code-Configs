    "javascript",
    "json",
    "scss"
  ],
  "dimmer.enabled": true,
  "dimmer.opacity": 65,
  "editor.wordWrap": "on",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.minimap.showSlider": "always",
  "workbench.preferredDarkColorTheme": "Dracula",
  "workbench.tree.indent": 4,
  "workbench.tree.renderIndentGuides": "always",
  "C_Cpp.updateChannel": "Insiders",
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": false,
  "editor.semanticHighlighting.enabled": true,
  "workbench.view.alwaysShowHeaderActions": true,
  // Modal layer;
  "modaledit.selectCursorStyle": "block-outline",
  "modaledit.searchCursorStyle": "underline",
  "startInNormalMode": "true",
  "modaledit.keybindings": {
    "u": "modaledit.enterInsert",
    "a": "workbench.action.showCommands",
    // Navigation
    "h": {
      "condition": "__selecting",
      "true": {
        "command": "cursorLeftSelect"
      },
      "false": "cursorLeft"
    },
    "n": {
      "condition": "__selecting",
      "true": "cursorRightSelect",
      "false": "cursorRight"
    },
    "H": {
      "condition": "__selecting",
      "true": "selectPartOfWord.selectLeft",
      "false": "selectPartOfWord.moveLeft"
    },
    "N": {
      "condition": "__selecting",
      "true": "selectPartOfWord.selectRight",
      "false": "selectPartOfWord.moveRight"
    },
    "t": {
      "condition": "__selecting",
      "true": "cursorDownSelect",
      "false": "cursorDown"
    },
    "c": {
      "condition": "__selecting",
      "true": "cursorUpSelect",
      "false": "cursorUp"
    },
    "C": {
      "condition": "__selecting",
      "false": "block-travel.jumpUp",
      "true": "block-travel.selectUp"
    },
    "T": {
      "condition": "__selecting",
      "false": "block-travel.jumpDown",
      "true": "block-travel.selectDown"
    },
    // Move by words.
    "r": {
      "condition": "__selecting",
      "false": {
        "command": "cursorWordStartRight",
      },
      "true": {
        "command": "cursorWordRightSelect",
      }
    },
    "g": {
      "condition": "__selecting",
      "false": {
        "command": "cursorWordStartLeft",
      },
      "true": {
        "command": "cursorWordLeftSelect",
      }
    },
    "R": {
      "condition": "__selecting",
      "false": {
        "command": "cursorWordStartRight",
        "repeat": "__char.match(/\\W/)"
      },
      "true": {
        "command": "cursorWordRightSelect",
        "repeat": "__char.match(/\\W/)"
      }
    },
    "G": {
      "condition": "__selecting",
      "false": {
        "command": "cursorWordStartLeft",
        "repeat": "__char.match(/\\W/)"
      },
      "true": {
        "command": "cursorWordLeftSelect",
        "repeat": "__char.match(/\\W/)"
      }
    },
    // Move by blocks.
    "s": {
      "condition": "__char == ''",
      "true": {
        "condition": "__selecting",
        "true": "block-travel.selectDown",
        "false": "block-travel.jumpDown"
      },
      "false": {
        "condition": "__selecting",
        "true": "cursorLineEndSelect",
        "false": "cursorLineEnd"
      }
    },
    "d": {
      "condition": "__col == 0",
      "true": {
        "condition": "__selecting",
        "true": "block-travel.selectUp",
        "false": "block-travel.jumpUp"
      },
      "false": {
        "condition": "__selecting",
        "true": "cursorLineStartSelect",
        "false": "smartBeginningOfLine"
      }
    },
    // Move to screen section.
    // "command": "cursorMove",
    // "args": "{ to: 'viewPortTop', select: __selecting }"
    //  "command": "cursorPageUp"
    //  },
    //  "T": {
    //  "command": "cursorMove",
    //  "command": "cursorPageDown",
    //  "args": "{ to: 'pageDown', select: __selecting }"
    //  "args": "{ to: 'viewPortBottom', select: __selecting }"
    //  },
    // ")": {
    //   "condition": "__selecting",
    //   "true": "editor.action.smartSelect.expand",
    //   "false": "editor.action.jumpToBracket"
    // },
    // "(": {
    //   "condition": "__selecting",
    //   "true": "editor.action.smartSelect.expand",
    //   "false": "editor.action.jumpToBracket"
    // },
    // Deletion
    ".": {
      "condition": "__selecting",
      "true": "modaledit.previousMatch",
      "false": "deleteWordLeft"
    },
    ">": "selectPartOfWord.backspace",
    "p": {
      "condition": "__selecting",
      "true": "modaledit.nextMatch",
      "false": "deleteWordRight"
    },
    "P": "selectPartOfWord.delete",
    "e": "extension.smartBackspace",
    "(": {
      "command": "deleteRight"
    },
    /*
      ",": [{
          "command": "deleteRight",
          "repeat": "__char.match(/\\s/)"
        },  {
          "command":"deleteLeft",
          "repeat":"__char.match(/\\s/)"
        }
      ],  */
    //  /*
    // ",": [
    //   "yo1dog.cursor-trim.trimCursor",
    //   "extension.hungryDelete"
    // ],
    // */
    "i": "deletehere.delete",
    // ";": "extension.embraceDoubleQuotes",
    "=": "editor.action.smartSelect.expand",
    // "=": "select-paragraph.selectParagraph",
    "*": "extension.selectPerfectLine",
    "+": "bracket-pair-colorizer-2.expandBracketSelection",
    // "+": "vscode-select-by-indent.select",
    ")": "extension.selectWord",
    ",": "yo1dog.cursor-trim.trimCursor",
    /*
           ",": [{
               "command": "modaledit.selectBetween",
               "args": {
                 "from": "'/s'",
                 "to":"'/s'"
               }
             },  {
               "command":"editor.action.deleteLines"
             }
           ],  */
    /*
       Searching
       "b": {
         "command:": "modaledit.search",
         "args": {
           "caseSensitive": false,
           "wrapAround": true
         }
      },
       "B": {
         "command:": "modaledit.search",
         "args": {
           "caseSensitive": true,
           "wrapAround": true
         }
       },
       */
    // Searching
    // "b": "references-view.next",
    // "B": "references-view.prev",
    // Cut
    // "q": [
    //   "copy-word.cut",
    //   "modaledit.cancelSelection"
    // ],
    "q": [
      {
        "condition": "__col == 0 || __selecting || __char == ''",
        "true": {
          "command": "editor.action.clipboardCutAction",
        },
        "false": [
          {
            "command": "copy-word.cut",
          },
          {
            "command": "editor.action.cancelSelectionAnchor"
          }
        ],
      },
      "modaledit.cancelSelection"
    ],
    // Copy
    "j": [
      {
        "condition": "__col == 0 || __selecting || __char == ''",
        "true": {
          "command": "editor.action.clipboardCopyAction",
        },
        "false": [
          {
            "command": "copy-word.copy",
          },
          {
            "command": "editor.action.cancelSelectionAnchor"
          }
        ],
      },
      "modaledit.cancelSelection"
    ],
    // Paste
    "k": [
      "editor.action.clipboardPasteAction",
      "modaledit.cancelSelection"
    ],
    "K": [
      "editor.action.clipboardPasteAction",
      "modaledit.cancelSelection"
    ],
    // Windows
    "}": {
      "command": "workbench.action.splitEditorDown"
    },
    "{": {
      "command": "workbench.action.joinAllGroups"
    },
    // Misc
    "'": "editor.action.commentLine",
    "y": "modaledit.toggleSelection",
    "f": "undo",
    ";": "redo",
    "w": "workbench.action.focusNextGroup",
    "W": "workbench.action.focusPreviousGroup",
    "_": "editor.togglecase",
    "-": "extension.toggle",
    "o": "smartsemicolon.insert",
    "O": "editor.action.insertLineAfter",
    // SPACEBAR STUFF
    " ": {
      // "i": "alm.blockCut",
      "a": "editor.action.selectAll",
      "s": "workbench.action.files.save",
      "k": "clipboardHistory.focus",
      "i": [
        "editor.action.deleteLines",
        "modaledit.cancelSelection"
      ],
      "m": "extension.dired.open",
      " ": "editor.action.showHover",
      "}": "workbench.action.splitEditorRight",
      "{": "workbench.action.closePanel",
      "r": "editor.toggleFold",
      "f": "editor.action.goToReferences",
      // "f": "references-view.showCallHierarchy",
      "F": "references-view.find",
      ".": {
        "w": "workbench.action.closeWindow",
        "r": "editor.foldAllMarkerRegions",
        "R": "editor.unfoldAllMarkerRegions",
      },
      "e": {
        "t": "extension.embraceParenthesis",
        "n": "extension.embraceSquareBrackets",
        "b": "extension.embraceCurlyBrackets",
        "g": "extension.embraceDoubleQuotes",
        "c": "extension.embraceSingleQuotes",
        "r": "extension.moveIntoRegion"
      },
      "c": {
        "u": "seito-openfile.openFileFromText",
        "i": "copyFilePath",
        ",": "extension.open",
        "n": "workbench.action.files.newUntitledFile",
        "o": "revealFileInOS",
        "l": "bookmarks.toggle",
        "c": "bookmarks.list",
        "f": "workbench.action.openRecent",
        "p": "workbench.action.reopenClosedEditor",
        ".": "workbench.action.files.openFile",
        ">": "workbench.action.files.openFolder",
        "s": "workbench.action.files.saveAs"
      },
      "t": {
        "b": "workbench.action.closeActiveEditor",
        "c": "center-editor-window.center",
        "g": "cursorPageUp",
        "m": "cursorPageDown",
      }
    }
  },
