	"tokenColors": [
		{
			"scope": [
				"emphasis"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"strong"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"header"
			],
			"settings": {
				"foreground": "#9580ff"
			}
		},
		{
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#f2f2f2"
			}
		},
		{
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#6272A4"
			}
		},
		{
			"scope": [
				"markup.inserted"
			],
			"settings": {
